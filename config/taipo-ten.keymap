/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &kp R   &kp S   &kp N   &kp I   &kp SPACE
                &kp A   &kp O   &kp T   &kp E   &kp BACKSPACE
